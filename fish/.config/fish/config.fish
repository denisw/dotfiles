# Disable autosuggestions
set -g fish_autosuggestion_enabled 0

# Git aliases
alias ga='git add'
alias gapa='git add --patch'
alias gc='git commit'
alias gc!='git commit --amend'
alias gco='git checkout'
alias gl='git pull'
alias gp='git push'
alias gp!='git push --force-with-lease'
alias gst='git status'
